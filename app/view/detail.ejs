<div id="consoleWrapper">
    <div class="layui-row layui-col-space10">
        <div class="layui-col-md8">
            <div class="layui-card">
                <div class="layui-card-header layui-elip">
                    <center>
                        <h1>
                            <%= rows.title %>
                        </h1>
                    </center>
                </div>
                <div class="layui-card-header">
                    <div class="layui-col-md4">来源：
                        <%= rows.source %>
                    </div>
                    <div class="layui-col-md4">来源站点：
                        <%= rows.website %>
                    </div>
                    <div class="layui-col-md4">创建时间：
                        <%= rows.create_time %>
                    </div>
                </div>
                <div class="layui-card-body layui-text">
                    <font face="verdana">
                        <%= rows.content %>
                    </font>
                </div>
            </div>
        </div>
        <!-- 网页右侧内容-->
        <div class="layui-col-md4">
            <!-- 关键词 -->
            <section>
                    <div class="layui-card">
                        <div class="layui-card-header">
                            <span class="layui-badge-dot layui-bg-blue mr-10"></span>关键词</div>
                        <div class="layui-card-body" style="padding: 0">
                            <div id="wordCloudWrapper" style="width: 100%;height:280px;"></div>
                        </div>
                    </div>
                </section>
            <!-- 热词云图 -->
            <section>
                <div class="layui-card">
                    <div class="layui-card-header">
                        <span class="layui-badge-dot layui-bg-blue mr-10"></span>热词云图</div>
                    <div class="layui-card-body" style="padding: 0">
                        <div id="wordCloudWrapper" style="width: 100%;height:280px;"></div>
                    </div>
                </div>
            </section>
        </div>
    </div>
</div>